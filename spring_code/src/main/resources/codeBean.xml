       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="codeBean" class="com.echo.feature.code.entity.CodeBean" init-method="init" destroy-method="destroy"/>
    <bean id="codeBeanPostProcess" class="com.echo.feature.code.process.CodeBeanPostProcess"/>
    <bean id="codeBeanFactoryPostProcess" class="com.echo.feature.code.process.CodeBeanFactoryPostProcess"/>
</beans>